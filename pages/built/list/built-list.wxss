@import "../../common/nav.wxss";
.swiper-content{
  height: 90rpx;
  line-height: 90rpx;
  width: 710rpx;
  margin: 0 auto;
  left: 0;
  top: 0;
  padding: 0 20rpx;
  position: fixed;
  overflow: hidden;
  background-color: #FFF;
  z-index: 3;
  white-space: nowrap;
}
.swiper-content scroll-view{
  font-size: 0;
}
.swiper-content view{
  font-size: 28rpx;
  display: inline-block;
  padding: 0 20rpx;
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  box-sizing: border-box;
}
.swiper-content view.active{
  font-weight: bold;
  border-bottom: 5rpx solid #000;
}
.swiper-content view.active:after{
  content: "";
  bottom: 0;
  display: block;
  left: 20rpx;
  right: 20rpx;
  border-bottom: 5rpx solid #000;
}
.built-list{
  background-color: #ededed;
  padding: 20rpx 20rpx 0 20rpx;
  margin-top: 90rpx;
}
.built-list .list{
  position: relative;
  border-radius: 12rpx;
  overflow: hidden;
  width: 100%;
  background-color: #FFF;
  margin-bottom: 20rpx;
}
.built-list .list .img{
  height: 314rpx;
  width: 100%;
  display: block;
}
.built-list .list:last-child{
  margin-bottom: 0;
}
.built-list .list .text{
  padding-top: 14rpx;
  line-height: 56rpx;
  font-size: 28rpx;
  padding-left: 28rpx;
}
.built-list .list .time-look{
  line-height: 30rpx;
  height: 30rpx;
  padding-bottom: 10rpx;
  padding-right: 16rpx;
  font-size: 20rpx;
  opacity: .6;
}
.built-list .list .time-look .look{
  padding-right: 26rpx;
  padding-left: 39rpx;
  position: relative;
}
.built-list .list .time-look .look image{
  position: absolute;
  left: 0;
  width: 32rpx;
  height: 20rpx;
  top: 5rpx;
}
.link-fixed{
  position: fixed;
  width: 91rpx;
  height: 91rpx;
  bottom: 120rpx;
  right: 20rpx;
  display: block;
}
.link-fixed image{
  width: 100%;
  height: 100%;
  display: block;
}